package com.model;

public class StudentJoin {
	private int id;
	private String studentName;
	private int cid;
	private String cName;
	private int cyear;
	private float score;
	
	public StudentJoin() {
		// TODO Auto-generated constructor stub
	}
	public StudentJoin(int sid, String sname, int clId, String cname, int clYear, float s)
	{
		id = sid;
		studentName = sname;
		cid = clId;
		cyear = clYear;
		score = s;
	}
	
	public String getStudentName() {
		return studentName;
	}
	public void setStudentName(String studentName) {
		this.studentName = studentName;
	}
	public int getStudentId() {
		return id;
	}
	public void setStudentId(int id) {
		this.id = id;
	}
	public int getClassId() {
		return cid;
	}
	public void setClassId(int cid) {
		this.cid = cid;
	}
	public String getClassName() {
		return cName;
	}
	public void setcClassName(String cName) {
		this.cName = cName;
	}
	public int getClassYear() {
		return cyear;
	}
	public void setClassYear(int cyear) {
		this.cyear = cyear;
	}
	public float getScore() {
		return score;
	}
	public void setScore(float score) {
		this.score = score;
	}
	
	public String toString()
	{
		return this.id + " " + this.studentName + " " + this.cid + " " + this.cName + " " + this.cyear + " " + this.score;
	}
}
